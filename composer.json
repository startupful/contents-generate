{
    "name": "startupful/contents-generate",
    "description": "You can manage the contents of various media by generating them.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Startupful\\ContentsGenerate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "startupful",
            "email": "one@startupful.io"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^10.0|^11.0",
        "smalot/pdfparser": "^2.2",
        "openai-php/laravel": "^0.10.1",
        "org_heigl/ghostscript": "^2.3",
        "phpoffice/phppresentation": "^1.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpoffice/phpword": "^1.3",
        "spatie/pdf-to-image": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "embed/embed": "^3.3",
        "google/apiclient": "^2.0",
        "php-ffmpeg/php-ffmpeg": "^1.2.0",
        "james-heinrich/getid3": "^1.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Startupful\\ContentsGenerate\\ContentsGenerateServiceProvider"
            ]
        },
        "migrations": [
            "database/migrations"
        ]
    }
}